{
  "name": "node-js-getting-started",
  "version": "0.1.5",
  "description": "A sample Node.js app using Express 4",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "server": "http-server ./public -p 2015",
    "jade": "jade --watch --pretty views/pages/index.jade --out ./public",
    "stylus": "stylus --use nib --watch views/styles/styles.styl --out ./public/stylesheets",
    "babel": "babel --watch lib --out-dir public",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {
    "ejs": "2.3.3",
    "express": "4.13.3",
    "jade": "~1.11.0",
    "stylus": "~0.52.4",
    "cool-ascii-faces": "~1.3.x"
  },

  "devDependencies": {
  "http-server": "~0.8.5",
  "babel": "~6.0.2",
  "normalize-styl": "~3.0.3",
  "nib": "~1.1.0"
  },

  "engines": {
    "node": "0.12.7"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/heroku/node-js-getting-started"
  },
  "keywords": [
    "node",
    "heroku",
    "express"
  ],
  "license": "MIT"
}
